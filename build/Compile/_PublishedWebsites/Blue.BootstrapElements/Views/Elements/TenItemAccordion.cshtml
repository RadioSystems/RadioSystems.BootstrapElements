@using Blue.BootstrapElements.Elements
@{
    var element = (TenItemAccordion) Model.Element;
}
<div class="@string.Format("panel-group {0}", element.HtmlClass)" id="@string.Format("{0}-accordion", element.HtmlId)" role="tablist" aria-multiselectable="true" style="@element.HtmlStyle">
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="@string.Format("{0}-heading-one", element.HtmlId)">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse"
                   onclick="toggleArrow(event)"
                   data-parent="@string.Format("#{0}-accordion", element.HtmlId)" href="@string.Format("#{0}-collapse-one", element.HtmlId)" aria-expanded="true" aria-controls="collapseOne">
                    <i class="fa @element.ItemOneIcon"></i>
                    @element.ItemOneTitle
                    <i class="fa fa-caret-up" style="float: right;"></i>
                </a>
            </h4>
        </div>
        <div id="@string.Format("{0}-collapse-one", element.HtmlId)" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="@element.ItemOneTitle">
            <div class="panel-body">
                @Html.Raw(element.ItemOneBody)
            </div>
        </div>
    </div>
